#!/bin/bash

if [ -f /deepinhost/etc/deepin-installer.conf ];then
    . /deepinhost/etc/deepin-installer.conf
fi

DI_LOCALE=${DI_LOCALE:-en_US}
CODENAME=${CODENAME:unstable}

case ${DI_LOCALE} in
    *)
        deepin_url="[trusted=yes] http://packages.deepin.com"
        debian_url="http://mirrors.ustc.edu.cn"
        ;;
esac

cat > /etc/apt/sources.list <<EOF
# Generated by deepin-installer
deb ${deepin_url}/mipsel raccoon main non-free contrib
EOF
